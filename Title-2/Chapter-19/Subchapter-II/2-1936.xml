<level type="section">
  <num>2-1936</num>
  <heading>Phased implementation.</heading>
  <level>
    <num>(a)</num>
    <text>This subchapter shall apply on June 19, 2004 to:</text>
    <level>
      <num>(1)</num>
      <text>Department of Health;</text>
    </level>
    <level>
      <num>(2)</num>
      <text>Department of Human Services;</text>
    </level>
    <level>
      <num>(3)</num>
      <text>Department of Employment Services;</text>
    </level>
    <level>
      <num>(4)</num>
      <text>Metropolitan Police Department;</text>
    </level>
    <level>
      <num>(5)</num>
      <text>District of Columbia Public School System;</text>
    </level>
    <level>
      <num>(6)</num>
      <text>Office of Planning;</text>
    </level>
    <level>
      <num>(7)</num>
      <text>Fire and Emergency Medical Services; and</text>
    </level>
    <level>
      <num>(8)</num>
      <text>Office of Human Rights.</text>
    </level>
  </level>
  <level>
